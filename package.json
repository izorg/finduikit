{
  "name": "finduikit",
  "private": true,
  "author": "Viacheslav Zavoruev<izorgster@gmail.com>",
  "type": "module",
  "sideEffects": [
    "domains/polyfills/**"
  ],
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "dev": "concurrently --names=\"firebase,next\" \"npm run dev-firebase\" \"npm run dev-next\"",
    "dev-firebase": "firebase emulators:start --export-on-exit --import=.firebase",
    "dev-next": "next dev",
    "format": "prettier --cache --ignore-unknown --write .",
    "generate-json-schema": "node --import=tsx --import=@nodejs-loaders/css-module domains/ui-kit/scripts/generate-json-schema.ts",
    "graphql-codegen": "graphql-codegen",
    "lint": "yarn lint-unused && yarn lint-format && yarn lint-types && yarn lint-js && yarn lint-css",
    "lint-css": "stylelint --allow-empty-input --cache \"**/*.css\"",
    "lint-format": "prettier --cache --check --ignore-unknown .",
    "lint-js": "eslint --cache",
    "lint-types": "tsc",
    "lint-unused": "knip --cache",
    "start": "next start",
    "update-ui-kits": "node --env-file-if-exists=.env.local --import=tsx --import=@nodejs-loaders/css-module domains/ui-kit/scripts/update-ui-kits.ts"
  },
  "dependencies": {
    "@mdi/js": "7.4.47",
    "@next/third-parties": "15.4.5",
    "@radix-ui/react-compose-refs": "1.1.2",
    "@radix-ui/themes": "3.2.1",
    "@sentry/nextjs": "10.0.0",
    "classnames": "2.5.1",
    "core-js": "3.44.0",
    "emoji-toolkit": "9.0.1",
    "firebase-admin": "13.4.0",
    "fuse.js": "7.1.0",
    "linkify-plugin-mention": "4.3.2",
    "linkify-react": "4.3.2",
    "linkifyjs": "4.3.2",
    "next": "15.4.5",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "simple-icons": "15.9.0",
    "usehooks-ts": "3.1.1"
  },
  "devDependencies": {
    "@eslint/css": "0.10.0",
    "@eslint/js": "9.32.0",
    "@eslint/json": "0.13.1",
    "@eslint/markdown": "7.1.0",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/near-operation-file-preset": "3.1.0",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@next/bundle-analyzer": "15.4.5",
    "@next/eslint-plugin-next": "15.4.5",
    "@nodejs-loaders/css-module": "1.1.0",
    "@types/eslint-plugin-jsx-a11y": "6.10.0",
    "@types/node": "22.17.0",
    "@types/react": "19.1.9",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "browserslist-config-baseline": "0.4.0",
    "concurrently": "9.2.0",
    "eslint": "9.32.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-json-schema-validator": "5.4.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-yml": "1.18.0",
    "firebase-tools": "14.11.2",
    "globals": "16.3.0",
    "graphql": "16.11.0",
    "knip": "5.62.0",
    "node-html-parser": "7.0.1",
    "postcss": "8.5.6",
    "postcss-import": "16.1.1",
    "postcss-preset-env": "10.2.4",
    "prettier": "3.6.2",
    "stylelint": "16.23.0",
    "stylelint-config-css-modules": "4.5.1",
    "stylelint-config-standard": "39.0.0",
    "stylelint-no-unsupported-browser-features": "8.0.4",
    "stylelint-order": "7.0.0",
    "tsx": "4.20.3",
    "typescript": "5.9.2",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "5.2.0",
    "yaml": "2.8.0",
    "zod": "4.0.14"
  }
}
