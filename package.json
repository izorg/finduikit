{
  "name": "finduikit",
  "private": true,
  "author": "Viacheslav Zavoruev<izorgster@gmail.com>",
  "type": "module",
  "sideEffects": false,
  "packageManager": "yarn@3.6.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "check-types": "tsc",
    "dev": "next dev",
    "format": "prettier --cache --ignore-unknown --write .",
    "lint-css": "stylelint --allow-empty-input --cache \"**/*.css\"",
    "lint-js": "eslint --cache \"**/*.{js,ts,tsx,yml}\"",
    "start": "next start",
    "update-ui-kits": "node --env-file=.env.local ./scripts/update-ui-kits.js && eslint --cache --fix \"ui-kits/*.yml\" && prettier --write ui-kits"
  },
  "dependencies": {
    "@adobe/react-spectrum": "3.30.0",
    "@react-spectrum/card": "3.0.0-alpha.22",
    "clsx": "2.0.0",
    "next": "13.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.3",
    "@types/node": "20.7.2",
    "@types/react": "18.2.23",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-perfectionist": "2.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-yml": "1.9.0",
    "postcss": "8.4.31",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "9.1.4",
    "prettier": "3.0.3",
    "stylelint": "15.10.3",
    "stylelint-config-css-modules": "4.3.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "typescript": "5.2.2",
    "yaml": "2.3.2",
    "zod": "3.22.2"
  }
}
