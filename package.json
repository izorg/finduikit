{
  "name": "finduikit",
  "private": true,
  "author": "Viacheslav Zavoruev<izorgster@gmail.com>",
  "type": "module",
  "sideEffects": false,
  "packageManager": "yarn@4.0.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "check-types": "tsc",
    "deploy": "npx firebase-tools deploy",
    "dev": "next dev",
    "format": "prettier --cache --ignore-unknown --write .",
    "lint-css": "stylelint --allow-empty-input --cache \"**/*.css\"",
    "lint-js": "eslint --cache \"**/*.{js,ts,tsx,yml}\"",
    "start": "npx http-server out --port=3000",
    "update-ui-kits": "node --env-file=.env.local ./scripts/update-ui-kits.js && eslint --cache --fix \"ui-kits/*.yml\" && prettier --write ui-kits"
  },
  "dependencies": {
    "@adobe/react-spectrum": "3.31.1",
    "@react-spectrum/card": "3.0.0-alpha.24",
    "@sentry/nextjs": "7.74.1",
    "fuse.js": "6.6.2",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.5.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.6",
    "@types/jsdom": "21.1.4",
    "@types/node": "20.8.7",
    "@types/react": "18.2.31",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-perfectionist": "2.2.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-yml": "1.10.0",
    "jsdom": "22.1.0",
    "postcss": "8.4.31",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "9.2.0",
    "prettier": "3.0.3",
    "stylelint": "15.11.0",
    "stylelint-config-css-modules": "4.3.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "typescript": "5.2.2",
    "yaml": "2.3.3",
    "zod": "3.22.4"
  }
}
