{
  "name": "finduikit",
  "private": true,
  "author": "Viacheslav Zavoruev<izorgster@gmail.com>",
  "type": "module",
  "sideEffects": [
    "domains/polyfills/**"
  ],
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "dev": "concurrently --names=\"firebase,next\" \"npm run dev-firebase\" \"npm run dev-next\"",
    "dev-firebase": "npx firebase-tools emulators:start --export-on-exit --import=.firebase",
    "dev-next": "next dev",
    "format": "prettier --cache --ignore-unknown --write .",
    "generate-json-schema": "node --import tsx --loader esm-loader-css domains/ui-kit/scripts/generate-json-schema.ts",
    "graphql-codegen": "graphql-codegen",
    "lint": "yarn lint-unused && yarn lint-format && yarn lint-types && yarn lint-js && yarn lint-css",
    "lint-css": "stylelint --allow-empty-input --cache \"**/*.css\"",
    "lint-format": "prettier --cache --check --ignore-unknown .",
    "lint-js": "eslint --cache",
    "lint-types": "tsc",
    "lint-unused": "knip --cache",
    "start": "next start",
    "update-ui-kits": "node --import tsx --loader esm-loader-css domains/ui-kit/scripts/update-ui-kits.ts"
  },
  "dependencies": {
    "@mdi/js": "7.4.47",
    "@next/third-parties": "15.2.4",
    "@radix-ui/themes": "3.2.1",
    "@sentry/nextjs": "9.11.0",
    "classnames": "2.5.1",
    "core-js": "3.41.0",
    "firebase-admin": "13.2.0",
    "fuse.js": "7.1.0",
    "next": "15.2.4",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "simple-icons": "14.12.1"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "4.6.0",
    "@next/bundle-analyzer": "15.2.4",
    "@next/eslint-plugin-next": "15.2.4",
    "@types/eslint-plugin-jsx-a11y": "6.10.0",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "babel-plugin-react-compiler": "19.0.0-beta-e993439-20250328",
    "concurrently": "9.1.2",
    "eslint": "9.24.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.3.1",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-json-schema-validator": "5.4.0",
    "eslint-plugin-jsonc": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.11.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-compiler": "19.0.0-beta-e993439-20250328",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-unicorn": "58.0.0",
    "eslint-plugin-yml": "1.17.0",
    "esm-loader-css": "1.0.6",
    "globals": "16.0.0",
    "graphql": "16.10.0",
    "knip": "5.47.0",
    "node-html-parser": "7.0.1",
    "postcss": "8.5.3",
    "postcss-import": "16.1.0",
    "postcss-preset-env": "10.1.5",
    "prettier": "3.5.3",
    "stylelint": "16.18.0",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-no-unsupported-browser-features": "8.0.4",
    "stylelint-order": "6.0.4",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.0",
    "typescript-plugin-css-modules": "5.1.0",
    "yaml": "2.7.1",
    "zod": "3.24.2",
    "zod-to-json-schema": "3.24.5"
  }
}
