{
  "name": "finduikit",
  "private": true,
  "author": "Viacheslav Zavoruev<izorgster@gmail.com>",
  "type": "module",
  "sideEffects": false,
  "packageManager": "yarn@4.4.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "check-types": "tsc",
    "deploy": "npx firebase-tools deploy",
    "dev": "next dev --turbo",
    "format": "prettier --cache --ignore-unknown --write .",
    "graphql-codegen": "graphql-codegen",
    "lint-css": "stylelint --allow-empty-input --cache \"**/*.css\"",
    "lint-js": "eslint --cache \"**/*.{js,ts,tsx,yml}\"",
    "start": "next start",
    "update-ui-kits": "node --env-file=.env.local ./scripts/update-ui-kits.js && eslint --cache --fix \"ui-kits/*.yml\" && prettier --write ui-kits"
  },
  "dependencies": {
    "@sentry/nextjs": "8.22.0",
    "fuse.js": "7.0.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript-operations": "4.2.3",
    "@next/bundle-analyzer": "14.2.5",
    "@types/node": "20.14.14",
    "@types/react": "18.3.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-perfectionist": "2.11.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-tailwindcss": "3.17.4",
    "eslint-plugin-yml": "1.14.0",
    "graphql": "16.9.0",
    "node-html-parser": "6.1.13",
    "postcss": "8.4.40",
    "postcss-import": "16.1.0",
    "postcss-nesting": "12.1.5",
    "postcss-preset-env": "9.6.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.5",
    "stylelint": "16.8.1",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.2",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4",
    "yaml": "2.5.0",
    "zod": "3.23.8"
  }
}
