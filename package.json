{
  "name": "finduikit",
  "private": true,
  "author": "Viacheslav Zavoruev<izorgster@gmail.com>",
  "type": "module",
  "sideEffects": false,
  "packageManager": "yarn@4.5.3",
  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --cache --ignore-unknown --write .",
    "graphql-codegen": "graphql-codegen",
    "lint": "yarn lint-format && yarn lint-types && yarn lint-js && yarn lint-css",
    "lint-css": "stylelint --allow-empty-input --cache \"**/*.css\"",
    "lint-format": "prettier --cache --check --ignore-unknown .",
    "lint-js": "eslint --cache \"**/*.{js,ts,tsx,yml}\"",
    "lint-types": "tsc",
    "start": "next start",
    "update-ui-kits": "node --experimental-strip-types scripts/update-ui-kits.ts && eslint --cache --fix \"ui-kits/*.yml\" && prettier --write ui-kits"
  },
  "dependencies": {
    "@mdi/js": "7.4.47",
    "@radix-ui/themes": "3.1.6",
    "@sentry/nextjs": "8.41.0",
    "classnames": "2.5.1",
    "fuse.js": "7.0.0",
    "next": "15.0.3",
    "next-themes": "0.4.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript-operations": "4.4.0",
    "@next/bundle-analyzer": "15.0.3",
    "@next/eslint-plugin-next": "15.0.3",
    "@types/node": "22.10.1",
    "@types/react": "18.3.12",
    "babel-plugin-react-compiler": "19.0.0-beta-df7b47d-20241124",
    "eslint": "9.15.0",
    "eslint-config-flat-gitignore": "0.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-compat": "6.0.1",
    "eslint-plugin-perfectionist": "4.1.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-compiler": "19.0.0-beta-df7b47d-20241124",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-yml": "1.15.0",
    "globals": "15.12.0",
    "graphql": "16.9.0",
    "node-html-parser": "6.1.13",
    "postcss": "8.4.49",
    "postcss-import": "16.1.0",
    "postcss-preset-env": "10.1.1",
    "prettier": "3.4.1",
    "stylelint": "16.11.0",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-no-unsupported-browser-features": "8.0.2",
    "stylelint-order": "6.0.4",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "typescript-plugin-css-modules": "5.1.0",
    "yaml": "2.6.1",
    "zod": "3.23.8"
  }
}
