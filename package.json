{
  "name": "finduikit",
  "private": true,
  "author": "Viacheslav Zavoruev<izorgster@gmail.com>",
  "type": "module",
  "sideEffects": false,
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "check-types": "tsc",
    "deploy": "npx firebase-tools deploy",
    "dev": "next dev",
    "format": "prettier --cache --ignore-unknown --write .",
    "graphql-codegen": "graphql-codegen",
    "lint-css": "stylelint --allow-empty-input --cache \"**/*.css\"",
    "lint-js": "eslint --cache \"**/*.{js,ts,tsx,yml}\"",
    "start": "next start",
    "update-ui-kits": "node --experimental-strip-types scripts/update-ui-kits.ts && eslint --cache --fix \"ui-kits/*.yml\" && prettier --write ui-kits"
  },
  "dependencies": {
    "@radix-ui/themes": "3.1.4",
    "@sentry/nextjs": "8.35.0",
    "fuse.js": "7.0.0",
    "next": "15.0.1",
    "next-themes": "0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript-operations": "4.3.0",
    "@next/bundle-analyzer": "15.0.1",
    "@next/eslint-plugin-next": "15.0.1",
    "@types/node": "22.8.1",
    "@types/react": "18.3.12",
    "eslint": "9.13.0",
    "eslint-config-flat-gitignore": "0.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-compat": "6.0.1",
    "eslint-plugin-perfectionist": "3.9.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unicorn": "56.0.0",
    "eslint-plugin-yml": "1.14.0",
    "globals": "15.11.0",
    "graphql": "16.9.0",
    "node-html-parser": "6.1.13",
    "postcss": "8.4.47",
    "postcss-import": "16.1.0",
    "postcss-preset-env": "10.0.8",
    "prettier": "3.3.3",
    "stylelint": "16.10.0",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-no-unsupported-browser-features": "8.0.1",
    "stylelint-order": "6.0.4",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0",
    "typescript-plugin-css-modules": "5.1.0",
    "webpack": "5.95.0",
    "yaml": "2.6.0",
    "zod": "3.23.8"
  }
}
